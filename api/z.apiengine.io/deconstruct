  status r
 session c cog orange

  method d cog Api_Engine 1 /status             GET
resource d cog Api_Engine 1 /status
  method d cog Api_Engine 1 /session                    POST
  method d cog Api_Engine 1 /session                    GET
resource d cog Api_Engine 1 /session
  method d cog Api_Engine 1 /session/{session}          DELETE
resource d cog Api_Engine 1 /session/{session}
  method d cog Api_Engine 1 /user                       POST
  method d cog Api_Engine 1 /user                       GET
resource d cog Api_Engine 1 /user
  method d cog Api_Engine 1 /user/{user}                GET
  method d cog Api_Engine 1 /user/{user}                UPDATE
  method d cog Api_Engine 1 /user/{user}                DELETE
resource d cog Api_Engine 1 /user/{user}
  method d cog Api_Engine 1 /passwordreset              POST
