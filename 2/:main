#!/bin/sh

loc=`dirname $0`
cmd=`basename $0`

cd $loc

. :bomb
. :string
. :http
. :specs
. :params
. :line
. :common $*
. ^filter
. ^collapse

shift


string_startswith "{" "{hi there}"
echo CONTAINS: $__
exit 5

line_available
line_locate
specs_split ${_line}
params_get "_args" ${_args}
params_match $# ${_args_m} ${_args_o}
echo COUNT: $#
echo LINE: ${_line}
echo MANDATORY: ${_args_m}
