#!/bin/sh

loc=`dirname $0`
cmd=`basename $0`

cd $loc

. :bomb
. :val
. :array
. :math
. :string
. :http
. :json
. :specs
. :param
. :line
. :req
. :url
. :common $*
. ^filter
. ^collapse

shift

. ^cmdline

line_locate
req_build
url_build
echo LINE: ${__line}
echo PAYLOAD: ${__payload}
echo URL: ${__url}
