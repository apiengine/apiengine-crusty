#!/bin/sh

set -o noglob
`cat matrix                              | \
sed s'+^\(.\{52\}\)\(.\)\(.*\)$+\1|\3+'  | \
sed s'+^\(.\{74\}\)\(.\)\(.*\)$+\1|\3+'  | \
sed s'+^\(.\{79\}\)\(.\)\(.*\)$+\1|\3+'  | \
sed s'+^\(.\{86\}\)\(.\)\(.*\)$+\1|\3+'  | \
sed s'+^\(.\{91\}\)\(.\)\(.*\)$+\1|\3+'  | \
sed s'+^\(.\{92\}\)\(.\)\(.*\)$+\1 \3+'  | \
sed s'+^\(.\{108\}\)\(.\)\(.*\)$+\1|\3+' | \
sed s'+^\(.\{153\}\)\(.\)\(.*\)$+\1|\3+' | \
sed s'+^\(.*\)$+|\1+'                    | \
sed s'+^\(.*\)$+\1|+'                    | \
cat                                      | \
sed s'+( )* + +g'                        | \
sed s'+| +|+g'                           | \
sed s'+ |+|+g'                           | \
cat > /tmp/poo`
#__split=" \
#+^\(.\{52\}\) \
#  \(.\{23\}\) \
#  \(.\{ 5\}\) \
#  \(.\{ 7\}\) \
#  \(.\{ 5\}\) \
#  \(.\{ 2\}\) \
#  \(.\{15\}\) \
#  \(.\{ 2\}\) \
#  \(.\{43\}\) \
#  \(.*\)$ \
#+ |\1|\2|\3|\4|\5|\7|\8|\9| \
#+"
#__split=`echo $__split | sed s'+ ++g'`
#echo $__split
