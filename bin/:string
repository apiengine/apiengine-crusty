#!/bin/sh

string_startswith() {
    if echo $1 | grep -q "^$2"; then
        __="true"
    else
        __="false"
    fi
}

string_endswith() {
    if echo $1 | grep -q "$2$"; then
        __="true"
    else
        __="false"
    fi
}

string_contains() {
    if echo $1 | grep -q "$2"; then
        __="true"
    else
        __="false"
    fi
}
