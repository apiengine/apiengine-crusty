#!/bin/sh

# NO LONGER USED!!!!!!!!!!!!!!!!!!!!!!!

url() {
    bomb_fail "NO LONGER USED"
    local injectable
    url_injectable ${__url}; injectable=$__
    url_parameters
    eval __url=${injectable}
}

url_injectable() {
    __=`echo $1 | sed 's,/#\([^/]*\),/${_\1_},g'`
}

url_parameters() {
    array_len "__args__m"; local len=$__
    local i=0
    while [ ${i} != ${len} ]; do
        array_get "__args__m" $i; local name=$__
        array_get "__cmdline" $i; local val=$__
        eval _${name}_=${val} 
        math_inc "i"
    done
}
